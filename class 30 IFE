//* (JavaScript) 
// ?Innediately Invoked Function Expression

const aFunction =function(a,b){
    console.log(`A simple Function ${a+b}`)
};

aFunction(3,4)
aFunction(7,5);

(function(a,b){
    console.log(a+b)
})(4,4);

((a,b)=>console.log(a*b))(3,5);
((a,b)=>console.log(a*b))(4,5);

