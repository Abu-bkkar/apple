let category = 'Motor Bike'
let carType;
switch(category){
    case `car`:
        carType= 'This a Car';
        break;
        case `Motor Bike`:
            carType = `This is a Motor Bike`;
            break;
            default:
                carType =`Unknown vahicle Cate gory`;

};
console.log(carType)
